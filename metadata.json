{
  "name": "sjoeboo-puppet",
  "version": "0.8.3",
  "author": "Matt Nicholson",
  "summary": "Module to manage puppet(agent(3)or 4), puppet-server",
  "license": "Apache-2.0",
  "source": "https://github.com/sjoeboo/puppet-puppet",
  "project_page": "https://github.com/sjoeboo/puppet-puppet",
  "issues_url": "https://github.com/sjoeboo/puppet-puppet/issues",
  "tags": ["puppet", "puppetserver"],
  "operatingsystem_support": [
    {
      "operatingsystem":"RedHat",
      "operatingsystemrelease":[ "6", "7" ]
    },
    {
      "operatingsystem":"CentOS",
      "operatingsystemrelease":[ "6", "7" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "12.04", "14.04" ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.2.0"
    }
  ],
  "dependencies": [
    { "name": "puppetlabs/concat", "version_requirement": ">= 1.1.0 <= 2.1.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">=4.2.0 <5.0.0" },
    { "name": "puppetlabs/firewall", "version_requirement": ">= 1.2.0 <2.0.0" }
  ]
}
